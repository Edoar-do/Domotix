        namespace regole {
            class src.domotix.model.bean.regole.Regola {
                - id : String
                - stato : boolean
                + Regola()
                + Regola()
                + Regola()
                + contieneSensore()
                + contieneAttuatore()
                + addAzione()
                + addCondizione()
                + addCondizone()
                + addOperatore()
                + esegui()
                + getAntecedente()
                + getConseguente()
                + getId()
                + getStato()
