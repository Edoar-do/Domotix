                {static} + UGUALE : String
                - operatore : String
                + Condizione()
                + getDestra()
                + getOperatore()
                + getSinistra()
                + toString()
                + valuta()
                - areNumeriche()
                - checkOperatore()
                - checkOperatoreNumerico()
            }

            interface src.domotix.model.bean.regole.InfoSensoriale {
                {abstract} + getValore()
            }

            class src.domotix.model.bean.regole.InfoCostante {
                - info : Object
                + InfoCostante()
                + getInfo()
