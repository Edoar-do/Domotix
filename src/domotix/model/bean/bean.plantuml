            class src.domotix.model.bean.regole.InfoVariabile {
                - nomeInfo : String
                + InfoVariabile()
                + getNomeInfo()
                + getSensore()
                + getValore()
                + toString()
            }

            class src.domotix.model.bean.regole.Conseguente {
                - azioni : List<Azione>
                + Conseguente()
                + addAzione()
                + esegui()
                + getAzioni()
                + toString()
            }

            class src.domotix.model.bean.regole.Azione {
                - parametri : List<Parametro>
                + Azione()
                + esegui()
                + getAttuatore()
                + getModalita()
                + getParametri()
                + toString()
                - checkParametri()
            }
